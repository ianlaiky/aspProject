<configuration>

    <system.web>
      <compilation debug="true" targetFramework="4.5" />
      <httpRuntime targetFramework="4.5" />
    </system.web>

  <!-- Add Configuration Connections string below here  -->
  <connectionStrings>
    <add name="HealthDBContext" connectionString="Data Source=(LocalDB)\ MSSQLLocalDB; AttachDbFilename=|DataDirectory|\HealthDB.mdf; Integrated Security=True"
    providerName="System.Data.SqlClient"
/>
  </connectionStrings>

  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>

</configuration>
